{
  "name": "mpapp",
  "version": "0.1.0",
  "license": "Multiplayr",
  "description": "Multiplayr headless platform using next.js",
  "engines": {
    "node": ">=12.10"
  },
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "NODE_ENV=production next start",
    "lint": "next lint",
    "dev:inspect": "NODE_OPTIONS='--inspect' next dev",
    "start-platformsh": "NODE_ENV=production next start -p $PORT",
    "prod": "npm run build && npm run start",
    "validate": "lint-staged"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run validate"
    }
  },
  "lint-staged": {
    "*.js": [
      "prettier --write"
    ]
  },
  "dependencies": {
    "@crystallize/content-transformer": "9.0.1",
    "@crystallize/grid-renderer": "2.1.1",
    "@crystallize/react-content-transformer": "2.0.0",
    "@crystallize/react-image": "7.3.1",
    "@crystallize/react-layout": "3.2.0",
    "@crystallize/react-video": "1.6.6",
    "@heroicons/react": "^1.0.5",
    "@stripe/react-stripe-js": "1.1.2",
    "@stripe/stripe-js": "1.8.0",
    "axios": "^0.24.0",
    "cssnano": "^4.1.10",
    "date-fns": "^2.27.0",
    "del": "^6.0.0",
    "form-data": "^4.0.0",
    "immer": "8.0.1",
    "js-cookie": "^3.0.1",
    "jsonwebtoken": "8.5.1",
    "lodash": "4.17.15",
    "next": "12.0.4",
    "next-auth": "^3.29.0",
    "next-i18next": "^9.2.0",
    "next-seo": "^4.17.0",
    "nookies": "^2.5.2",
    "postcss": "^8.2.8",
    "prop-types": "15.7.2",
    "rc-slider": "9.5.4",
    "react": "^17.0.2",
    "react-date-range": "^1.4.0",
    "react-datepicker": "^4.4.0",
    "react-dom": "17.0.2",
    "react-hook-form": "^7.17.2",
    "react-is": "17.0.2",
    "react-number-format": "^4.8.0",
    "react-query": "^3.32.1",
    "react-timecalendar": "^2.1.3",
    "react-toastify": "^8.0.2",
    "stripe": "8.73.0",
    "styled-components": "5.3.0",
    "styled-is": "1.3.0",
    "uuid": "3.3.2"
  },
  "devDependencies": {
    "@babel/core": "7.9.6",
    "@crystallize/import-utilities": "0.10.4",
    "eslint": "8.0.0",
    "eslint-config-next": "11.1.2",
    "css-loader": "^5.1.1",
    "style-loader": "^2.0.0",
    "eslint-config-prettier": "6.11.0",
    "eslint-plugin-prettier": "3.1.0",
    "fs-extra": "9.0.1",
    "husky": "4.2.1",
    "lint-staged": "10.2.2",
    "node-fetch": "2.6.1",
    "prettier": "2.0.5"
  }
}
